<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <appSettings>
      <!--Server options-->
      <add key="ExchangeVersion" value="Exchange2010_SP2" />

      <!--User credentials-->
      <add key="UserName" value="" />
      <add key="Password" value="" />
      <add key="UserDomain" value="" />

      <!--Delivery options-->
      <add key="EmailDomain" value="" />

      <!--Email options-->
      <add key="Subject" value="Test subject" />
      <add key="Body" value="Test body" />
      <add key="FileExtention" value="*.xlsx"/>


      <!--Development options-->
      <add key="Trace" value="false" />
      <add key="Logging" value="true" />
    </appSettings>
</configuration>